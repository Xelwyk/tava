{
  "name": "tava",
	"displayName": "T.A.V.A.",
	"description": "Timestamp Analysis Visual Aid",
	"version": "1.0.2",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tava.setTargetInterval",
				"title": "Tava: Set Target Interval"
			}
		],
		"configuration":[
			{
				"title": "Tava",
				"properties": {
					"tava.targetInterval": {
						"type": "integer",
						"default": 60000,
						"description": "if time difference between timestamps is bigger than this value it changes color for coloring timestamps"
					}
				}
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Xelwyk/tava.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.65.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.2"
	}
}
